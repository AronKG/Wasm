    "antlr4": "antlr4 -Dlanguage=Cpp SimpleGrammar.g4",
    "antlr5": "java -jar //wsl.localhost/Ubuntu/usr/bin/antlr-4.9.2-complete.jar -Dlanguage=Cpp YourGrammar.g4",
