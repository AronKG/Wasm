    "compile3": "emcc lib3/example.cpp -o lib3/example.js -s WASM_ASYNC_COMPILATION=0 -std=c++11 --bind",
    "start3": "node --experimental-wasm-modules lib3/consumer.mjs",

    "compile4": "emcc property-filter-wasm/property-filter-wasm.cpp -o property-filter-wasm/property-filter-wasm.js -s WASM_ASYNC_COMPILATION=0 -std=c++11 --bind",
